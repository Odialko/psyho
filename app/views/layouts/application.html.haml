!!!
%html
  %head
    %meta(name="viewport" content="width=device-width, initial-scale=1.0")
    %title PsychoCabinets
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.bundle.min.js'

    -#include Bootstrap
    %link{href: "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css", rel: "stylesheet", integrity: "sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u", crossorigin: "anonymous"}
    %script{src: "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js", integrity: "sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa", crossorigin: "anonymous"}

    -#include Slick
    = stylesheet_link_tag '//cdn.jsdelivr.net/jquery.slick/1.6.0/slick.css'
    = stylesheet_link_tag '//cdn.jsdelivr.net/jquery.slick/1.6.0/slick-theme.css'
    -#%link{rel: "stylesheet", type: "text/css", href: "//cdn.jsdelivr.net/jquery.slick/1.6.0/slick.css"}
    %script{type: "text/javascript", src: "//cdn.jsdelivr.net/jquery.slick/1.6.0/slick.min.js"}

  %body
    -#= render 'layouts/header_picture'
    = yield

    - if request.path.eql? root_path
      = render partial: 'layouts/footer'